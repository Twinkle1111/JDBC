  	<servlet-name>IEmployeeRepositary</servlet-name>
  	<url-pattern>/iemployeerepositary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeServiceImpl</servlet-name>
  	<url-pattern>/employeeserviceimpl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IEmloyeeService</servlet-name>
