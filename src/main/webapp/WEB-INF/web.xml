  	<description></description>
  	<servlet-class>com.java.utils.ConnectionUtils</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TestConnection</servlet-name>
  	<display-name>TestConnection</display-name>
  	<description></description>
  	<servlet-class>com.java.utils.TestConnection</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>App</servlet-name>
  	<display-name>App</display-name>
  	<description></description>
  	<servlet-class>com.java.App</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeRepositaryImpl</servlet-name>
  	<display-name>EmployeeRepositaryImpl</display-name>
  	<description></description>
  	<servlet-class>com.java.repositary.EmployeeRepositaryImpl</servlet-class>
  </servlet>
