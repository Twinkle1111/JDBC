  	<servlet-class>com.java.service.IEmloyeeService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeQueries</servlet-name>
  	<display-name>EmployeeQueries</display-name>
  	<description></description>
  	<servlet-class>com.java.utils.EmployeeQueries</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Model</servlet-name>
  	<url-pattern>/model</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConnectionUtils</servlet-name>
  	<url-pattern>/ConnectionUtils</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TestConnection</servlet-name>
  	<url-pattern>/testConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>App</servlet-name>
